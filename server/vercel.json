{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm run build",
                "startCommand": "npm run start"
              }
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
       {
            "src": "/(.*)", "dest": "/api"
       }
    ]
}